#!/bin/bash --login

###
# job name
#SBATCH --job-name=gpc_network_test
# job stdout file
#SBATCH --output=gpc_network_test.%J.out
# job stderr file
#SBATCH --error=gpc_network_test.%J.err
# On failure, requeue for another try
#SBATCH --requeue
# maximum job time in HH:MM:SS
#SBATCH --time=06:00:00
#SBATCH --nodes=10
# maximum memory
#SBATCH --mem-per-cpu=1024
# run a single task
#SBATCH --ntasks=512
#SBATCH --cpus-per-task=1
###

module load singularity/3.4.1
cd /home/users/msrinivasa/develop/GPCNET

srun network_test
